a = {\n            'type': 'notename', \n            'token': this.token,\n            'data': this.notename\n        }\n        data = JSON.stringify(data)\n        let params = {'data': AES.Encrypt(data)}\n        http.post('/api/query', params).then((res) => {\n            res.data = eval('(' + res.data + ')')\n            switch(res.data.code ){\n                case 'Z1001':\n                this.$Notice.error({\n                  title: '系统异常',\n                  desc: '系统发生异常,请稍后再次尝试'\n                })\n                break\n                case 'Z1002':\n                this.$Notice.error({\n                  title: '系统异常',\n                  desc: '系统发生异常,请稍后再次尝试'\n                })\n                break\n                case 'Z10010':\n                this.flag = '1'\n                this.$Notice.error({\n                  title: '笔记已存在',\n                  desc: '笔记名字已存在,请重新输入笔记名字'\n                })\n                default:\n                break\n            }\n        })\n      }\n    },\n    change (status) {\n      if (status){\n        this.publish_flag = '1'\n      }\n      else{\n        this.publish_flag = '0'\n      }\n    },\n    handleSubmit () {\n      this.$emit('on-success-valid', {\n        notename: this.notename,\n        content: this.content,\n        flag: this.flag,\n        publish_flag: this.publish_flag,\n        token: this.token\n      })\n    },\n    $imgAdd(pos, $file){\n            // 第一步.将图片上传到服务器.\n          var formdata = new FormData();\n          formdata.append('image', $file);\n          axios({\n              url: '127.0.0.1:5000/upload',\n              method: 'post',\n              data: formdata,\n              headers: { 'Content-Type': 'multipart/form-data' },\n          }).then((url) => {\n              // 第二步.将返回的url替换到文本原位置![...](0) -> ![...](url)\n              // $vm.$img2Url 详情见本页末尾\n              $vm.$img2Url(pos, url);\n          })\n    },\n    $imgDel(pos, $file){\n            // 第一步.将图片上传到服务器.\n          var formdata = new FormData();\n          formdata.append('image', $file);\n          axios({\n              url: '127.0.0.1:5000/upload',\n              method: 'post',\n              data: formdata,\n              headers: { 'Content-Type': 'multipart/form-data' },\n          }).then((url) => {\n              // 第二步.将返回的url替换到文本原位置![...](0) -> ![...](url)\n              // $vm.$img2Url 详情见本页末尾\n              $vm.$img2Url(pos, url);\n          })\n    }\n  },\n  mounted () {\n  }\n}\n</script>\n\n<style>\n    .editor {\n      margin-top:  30px !important; \n    }\n    .button {\n       margin-top:  30px !important; \n    }\n</style>"],"sourceRoot":"src/components/markdown"}]}